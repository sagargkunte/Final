<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Healthcare Portal</title>
  <!-- Tailwind CDN -->
  <script src="https://cdn.tailwindcss.com"></script>
  <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/7.0.1/css/all.min.css"
    integrity="sha512-2SwdPD6INVrV/lHTZbO2nodKhrnDdJK9/kg2XD1r9uGqPo1cUbujc+IYdlYdEErWNu69gVcYgdxlmVmzTWnetw=="
    crossorigin="anonymous"
    referrerpolicy="no-referrer"
  />
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap');
    
    body {
      font-family: 'Inter', sans-serif;
    }
    
    .tab-btn {
      position: relative;
      overflow: hidden;
      transition: all 0.3s ease;
    }
    
    .tab-btn.active::after {
      content: '';
      position: absolute;
      bottom: 0;
      left: 50%;
      transform: translateX(-50%);
      width: 80%;
      height: 3px;
      background: linear-gradient(90deg, #3b82f6, #8b5cf6);
      border-radius: 3px;
    }
    
    .form-container {
      transition: all 0.4s ease;
    }
    
    .input-field {
      transition: all 0.2s ease;
    }
    
    .input-field:focus {
      box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.1);
    }
    
    .btn-primary {
      background: linear-gradient(135deg, #3b82f6, #6366f1);
      transition: all 0.3s ease;
    }
    
    .btn-primary:hover {
      background: linear-gradient(135deg, #2563eb, #4f46e5);
      transform: translateY(-2px);
      box-shadow: 0 10px 20px rgba(59, 130, 246, 0.3);
    }
    
    .btn-secondary {
      background: linear-gradient(135deg, #10b981, #059669);
      transition: all 0.3s ease;
    }
    
    .btn-secondary:hover {
      background: linear-gradient(135deg, #059669, #047857);
      transform: translateY(-2px);
      box-shadow: 0 10px 20px rgba(16, 185, 129, 0.3);
    }
    
    .otp-input {
      letter-spacing: 10px;
      font-weight: 600;
    }
    
    .glass-effect {
      backdrop-filter: blur(10px);
      background: rgba(255, 255, 255, 0.85);
    }
    
    .pulse-animation {
      animation: pulse 2s infinite;
    }
    
    @keyframes pulse {
      0% {
        box-shadow: 0 0 0 0 rgba(59, 130, 246, 0.4);
      }
      70% {
        box-shadow: 0 0 0 10px rgba(59, 130, 246, 0);
      }
      100% {
        box-shadow: 0 0 0 0 rgba(59, 130, 246, 0);
      }
    }
  </style>
</head>

<body class="min-h-screen bg-gradient-to-br from-blue-50 via-indigo-50 to-purple-50 flex justify-center items-center p-6">
  <div class="glass-effect shadow-2xl rounded-2xl w-full max-w-md overflow-hidden transition-all duration-300 border border-white/50">
    <!-- Header with Logo -->
    <div class="bg-gradient-to-r from-blue-600 to-indigo-600 p-6 text-center text-white">
      <div class="flex justify-center mb-4">
        <div class="w-12 h-12 rounded-full bg-white/20 flex items-center justify-center">
          <i class="fas fa-heartbeat text-xl"></i>
        </div>
      </div>
      <h1 class="text-2xl font-bold">Healthcare Portal</h1>
      <p class="text-blue-100 mt-2">Your health, our priority</p>
    </div>
    
    <!-- Tabs -->
    <div class="flex justify-between items-center p-4 bg-white/80">
      <button id="adminBtn"
        class="tab-btn active flex-1 py-3 text-sm sm:text-base font-semibold text-blue-600 rounded-lg transition-all duration-200">
        <i class="fas fa-user-shield mr-2"></i>Admin
      </button>
      <button id="doctorBtn"
        class="tab-btn flex-1 py-3 text-sm sm:text-base font-medium text-gray-600 rounded-lg transition-all duration-200">
        <i class="fas fa-user-md mr-2"></i>Doctor
      </button>
      <button id="patientBtn"
        class="tab-btn flex-1 py-3 text-sm sm:text-base font-medium text-gray-600 rounded-lg transition-all duration-200">
        <i class="fas fa-user-injured mr-2"></i>Patient
      </button>
    </div>

    <!-- Forms Container -->
    <div class="p-6">
      <!-- Admin Login -->
      <form id="adminForm" class="form-container space-y-5" action="/admin" method="POST">
        <h1 class="text-2xl font-bold text-center text-gray-800 mb-2">Admin Login</h1>
        <p class="text-center text-gray-500 mb-6">Access administrative dashboard</p>
        
        <div>
          <label for="adminEmail" class="block text-gray-700 font-medium mb-2">Email Address</label>
          <div class="relative">
            <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
              <i class="fas fa-envelope text-gray-400"></i>
            </div>
            <input type="email" id="adminEmail" placeholder="Enter your email"
              class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="email"/>
          </div>
        </div>
        
        <div>
          <label for="adminPassword" class="block text-gray-700 font-medium mb-2">Password</label>
          <div class="relative">
            <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
              <i class="fas fa-lock text-gray-400"></i>
            </div>
            <input type="password" id="adminPassword" placeholder="Enter your password"
              class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="password"/>
          </div>
        </div>
        
        <div class="flex items-center justify-between">
          <div class="flex items-center">
            <input type="checkbox" id="rememberMe" class="h-4 w-4 text-blue-600 focus:ring-blue-500 border-gray-300 rounded">
            <label for="rememberMe" class="ml-2 block text-sm text-gray-700">Remember me</label>
          </div>
          <a href="#" class="text-sm text-blue-600 hover:text-blue-800">Forgot password?</a>
        </div>
        
        <button
          class="btn-primary w-full text-white font-semibold rounded-lg py-3 transition duration-200">
          <i class="fas fa-sign-in-alt mr-2"></i>Login
        </button>
      </form>

      <!-- Doctor Register -->
      <form id="doctorForm" class="form-container hidden space-y-5">
        <h1 class="text-2xl font-bold text-center text-gray-800 mb-2">Doctor Registration</h1>
        <p class="text-center text-gray-500 mb-6">Join our medical network</p>
        
        <div id="doctorRegMessage" class="hidden p-4 rounded-lg mb-4"></div>
        
        <div class="grid grid-cols-1 gap-5">
          <div>
            <label for="docName" class="block text-gray-700 font-medium mb-2">Full Name</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-user text-gray-400"></i>
              </div>
              <input type="text" id="docName" placeholder="Enter your full name" required
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="name"/>
            </div>
          </div>
          
          <div>
            <label for="docEmail" class="block text-gray-700 font-medium mb-2">Email Address</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-envelope text-gray-400"></i>
              </div>
              <input type="email" id="docEmail" placeholder="Enter your email" required
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="email"/>
            </div>
          </div>
          
          <div>
            <label for="docPassword" class="block text-gray-700 font-medium mb-2">Password</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-lock text-gray-400"></i>
              </div>
              <input type="password" id="docPassword" placeholder="Enter your password" required minlength="6"
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="password"/>
            </div>
          </div>
          
          <div>
            <label for="docPhone" class="block text-gray-700 font-medium mb-2">Phone Number</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-phone text-gray-400"></i>
              </div>
              <input type="text" id="docPhone" placeholder="Enter your phone number"
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="phone"/>
            </div>
          </div>
          
          <div>
            <label for="Location" class="block text-gray-700 font-medium mb-2">Location</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-map-marker-alt text-gray-400"></i>
              </div>
              <input type="text" id="Location" placeholder="Enter your location"
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="location"/>
            </div>
          </div>
          
          <div>
            <label for="Specialization" class="block text-gray-700 font-medium mb-2">Specialization</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-stethoscope text-gray-400"></i>
              </div>
              <input type="text" id="Specialization" placeholder="Enter your specialization"
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none" name="specialization"/>
            </div>
          </div>
          
          <fieldset class="flex gap-6">
            <legend class="block text-gray-700 font-medium mb-2">Gender</legend>
            <label class="flex items-center text-gray-700 font-medium">
              <input type="radio" name="gender" class="h-4 w-4 text-blue-600 focus:ring-blue-500 border-gray-300 mr-2" value="male" required/> Male
            </label>
            <label class="flex items-center text-gray-700 font-medium">
              <input type="radio" name="gender" class="h-4 w-4 text-blue-600 focus:ring-blue-500 border-gray-300 mr-2" value="female" required/> Female
            </label>
          </fieldset>
        </div>
        
        <p class="text-sm text-center text-gray-600 mt-4">
          Already Registered?
          <a href="/doctorLogin" class="text-blue-600 hover:text-blue-800 font-medium">Login here</a>
        </p>
        
        <button type="submit"
          class="btn-primary w-full text-white font-semibold rounded-lg py-3 transition duration-200">
          <i class="fas fa-user-plus mr-2"></i>Register
        </button>
      </form>

      <!-- Patient Register -->
      <div id="patientForm" class="form-container hidden space-y-5">
        <h1 class="text-2xl font-bold text-center text-gray-800 mb-2">Patient Registration</h1>
        <p class="text-center text-gray-500 mb-6">Get started with your healthcare journey</p>
        
        <div id="patientMessage" class="hidden p-4 rounded-lg mb-4"></div>
        
        <!-- Step 1: Email Input -->
        <div id="emailStep">
          <div>
            <label for="patientEmail" class="block text-gray-700 font-medium mb-2">Email Address *</label>
            <div class="relative">
              <div class="absolute inset-y-0 left-0 pl-3 flex items-center pointer-events-none">
                <i class="fas fa-envelope text-gray-400"></i>
              </div>
              <input type="email" id="patientEmail" placeholder="Enter your email ID" required
                class="input-field w-full border border-gray-300 rounded-lg pl-10 pr-3 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none"/>
            </div>
          </div>
          
          <button type="button" id="sendOtpBtn"
            class="btn-primary w-full text-white font-semibold rounded-lg py-3 transition duration-200 mt-4">
            <i class="fas fa-paper-plane mr-2"></i>Send OTP
          </button>
        </div>

        <!-- Step 2: OTP Verification -->
        <div id="otpStep" class="hidden">
          <div class="text-center mb-4">
            <div class="w-16 h-16 rounded-full bg-blue-100 flex items-center justify-center mx-auto mb-3">
              <i class="fas fa-key text-blue-600 text-xl"></i>
            </div>
            <p class="text-gray-600">We've sent a verification code to your email</p>
            <p class="font-medium text-gray-800 mt-1">${patientEmailValue}</p>
          </div>
          
          <div>
            <label for="otpInput" class="block text-gray-700 font-medium mb-2">Enter OTP *</label>
            <input type="text" id="otpInput" placeholder="Enter 6-digit OTP" maxlength="6" required
              class="otp-input w-full border border-gray-300 rounded-lg px-4 py-3 focus:ring-2 focus:ring-blue-400 focus:outline-none text-center text-xl tracking-widest"/>
            <p class="text-xs text-gray-500 mt-2 text-center">Check your email for the OTP code</p>
          </div>
          
          <button type="button" id="verifyOtpBtn"
            class="btn-secondary w-full text-white font-semibold rounded-lg py-3 transition duration-200 mt-4">
            <i class="fas fa-check mr-2"></i>Verify OTP & Register
          </button>
          
          <button type="button" id="resendOtpBtn"
            class="w-full bg-gray-100 hover:bg-gray-200 text-gray-800 font-medium rounded-lg py-3 transition duration-200 mt-2">
            <i class="fas fa-redo mr-2"></i>Resend OTP
          </button>
          
          <button type="button" id="backToEmailBtn"
            class="w-full text-blue-600 hover:text-blue-800 font-medium py-2 transition duration-200">
            <i class="fas fa-arrow-left mr-2"></i>Back to Email
          </button>
        </div>

        <div class="relative my-6">
          <div class="absolute inset-0 flex items-center">
            <div class="w-full border-t border-gray-300"></div>
          </div>
          <div class="relative flex justify-center text-sm">
            <span class="px-3 bg-white text-gray-500 font-medium">OR</span>
          </div>
        </div>

        <div
            class="text-center border-2 border-blue-600 bg-white hover:bg-blue-50 py-3 rounded-lg transition cursor-pointer pulse-animation"
            onclick="GoogleAuth()"
          >
            <i class="fa-brands fa-google mr-2 text-blue-600"></i> 
            <span class="text-blue-600 font-semibold">Login with Google</span>
          </div>
      </div>
    </div>
  </div>

  <!-- JavaScript for toggling -->
  <script>
    const adminBtn = document.getElementById('adminBtn');
    const doctorBtn = document.getElementById('doctorBtn');
    const patientBtn = document.getElementById('patientBtn');

    const adminForm = document.getElementById('adminForm');
    const doctorForm = document.getElementById('doctorForm');
    const patientForm = document.getElementById('patientForm');

    const buttons = [adminBtn, doctorBtn, patientBtn];
    const forms = [adminForm, doctorForm, patientForm];

    function showForm(index) {
      forms.forEach((form, i) => {
        form.classList.toggle('hidden', i !== index);
        buttons[i].classList.toggle('active', i === index);
        buttons[i].classList.toggle('text-blue-600', i === index);
        buttons[i].classList.toggle('font-semibold', i === index);
        buttons[i].classList.toggle('text-gray-600', i !== index);
        buttons[i].classList.toggle('font-medium', i !== index);
      });
    }

    adminBtn.addEventListener('click', () => showForm(0));
    doctorBtn.addEventListener('click', () => showForm(1));
    patientBtn.addEventListener('click', () => showForm(2));

    // Doctor Registration Form Handler
    doctorForm.addEventListener('submit', async function(e) {
      e.preventDefault();
      
      const messageDiv = document.getElementById('doctorRegMessage');
      const submitBtn = this.querySelector('button[type="submit"]');
      const originalText = submitBtn.textContent;
      
      // Show loading
      submitBtn.disabled = true;
      submitBtn.textContent = 'Registering...';
      messageDiv.classList.add('hidden');
      
      // Get form data
      const formData = {
        name: document.getElementById('docName').value,
        email: document.getElementById('docEmail').value,
        password: document.getElementById('docPassword').value,
        phone: document.getElementById('docPhone').value,
        location: document.getElementById('Location').value,
        specialization: document.getElementById('Specialization').value,
        gender: this.querySelector('input[name="gender"]:checked').value
      };
      
      try {
        const response = await fetch('/doctor/register', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(formData)
        });
        
        const data = await response.json();
        
        if (data.success) {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-green-100 border border-green-400 text-green-700';
          messageDiv.textContent = data.message;
          messageDiv.classList.remove('hidden');
          this.reset();
          
          // Redirect to login after 2 seconds
          setTimeout(() => {
            window.location.href = '/doctorLogin';
          }, 2000);
        } else {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
          messageDiv.textContent = data.message || 'Registration failed. Please try again.';
          messageDiv.classList.remove('hidden');
          submitBtn.disabled = false;
          submitBtn.textContent = originalText;
        }
      } catch (error) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Error registering. Please try again.';
        messageDiv.classList.remove('hidden');
        submitBtn.disabled = false;
        submitBtn.textContent = originalText;
      }
    });

    // Patient Registration - OTP Flow
    let patientEmailValue = '';

    document.getElementById('sendOtpBtn')?.addEventListener('click', async function() {
      const emailInput = document.getElementById('patientEmail');
      const email = emailInput.value.trim();
      const messageDiv = document.getElementById('patientMessage');
      const btn = this;
      const originalText = btn.innerHTML;

      if (!email) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Please enter your email address';
        messageDiv.classList.remove('hidden');
        return;
      }

      // Validate email format
      const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
      if (!emailRegex.test(email)) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Please enter a valid email address';
        messageDiv.classList.remove('hidden');
        return;
      }

      patientEmailValue = email;
      btn.disabled = true;
      btn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i>Sending...';
      messageDiv.classList.add('hidden');

      try {
        const response = await fetch('/patientPage/send-otp', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ email })
        });

        const data = await response.json();

        if (data.success) {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-green-100 border border-green-400 text-green-700';
          messageDiv.textContent = data.message;
          messageDiv.classList.remove('hidden');
          
          // Show OTP step
          document.getElementById('emailStep').classList.add('hidden');
          document.getElementById('otpStep').classList.remove('hidden');
          
          // Update email display in OTP step
          const emailDisplay = document.querySelector('#otpStep p.font-medium');
          if (emailDisplay) {
            emailDisplay.textContent = patientEmailValue;
          }
        } else {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
          let errorMsg = data.message || 'Failed to send OTP. Please try again.';
          if (data.details) {
            errorMsg += ' (Error: ' + data.details + ')';
          }
          messageDiv.textContent = errorMsg;
          messageDiv.classList.remove('hidden');
          btn.disabled = false;
          btn.innerHTML = originalText;
        }
      } catch (error) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Error sending OTP. Please try again.';
        messageDiv.classList.remove('hidden');
        btn.disabled = false;
        btn.innerHTML = originalText;
      }
    });

    document.getElementById('verifyOtpBtn')?.addEventListener('click', async function() {
      const otpInput = document.getElementById('otpInput');
      const otpCode = otpInput.value.trim();
      const messageDiv = document.getElementById('patientMessage');
      const btn = this;
      const originalText = btn.innerHTML;

      if (!otpCode || otpCode.length !== 6) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Please enter a valid 6-digit OTP';
        messageDiv.classList.remove('hidden');
        return;
      }

      btn.disabled = true;
      btn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i>Verifying...';
      messageDiv.classList.add('hidden');

      try {
        const response = await fetch('/patientPage/verify-otp', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ 
            email: patientEmailValue,
            otpCode: otpCode
          })
        });

        const data = await response.json();

        if (data.success) {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-green-100 border border-green-400 text-green-700';
          messageDiv.textContent = data.message;
          messageDiv.classList.remove('hidden');
          
          // Redirect to patient page
          setTimeout(() => {
            window.location.href = data.redirectUrl || '/patientPage';
          }, 1500);
        } else {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
          messageDiv.textContent = data.message || 'Invalid OTP. Please try again.';
          messageDiv.classList.remove('hidden');
          btn.disabled = false;
          btn.innerHTML = originalText;
          otpInput.value = '';
        }
      } catch (error) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Error verifying OTP. Please try again.';
        messageDiv.classList.remove('hidden');
        btn.disabled = false;
        btn.innerHTML = originalText;
      }
    });

    document.getElementById('resendOtpBtn')?.addEventListener('click', async function() {
      const resendBtn = this;
      const messageDiv = document.getElementById('patientMessage');
      const originalText = resendBtn.innerHTML;
      
      // Clear OTP input
      document.getElementById('otpInput').value = '';
      
      // Show loading state
      resendBtn.disabled = true;
      resendBtn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i>Sending...';
      messageDiv.classList.add('hidden');

      try {
        const response = await fetch('/patientPage/send-otp', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ email: patientEmailValue })
        });

        const data = await response.json();

        if (data.success) {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-green-100 border border-green-400 text-green-700';
          messageDiv.textContent = 'New OTP sent successfully! Please check your email.';
          messageDiv.classList.remove('hidden');
        } else {
          messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
          messageDiv.textContent = data.message || 'Failed to resend OTP. Please try again.';
          messageDiv.classList.remove('hidden');
        }
      } catch (error) {
        messageDiv.className = 'p-4 rounded-lg mb-4 bg-red-100 border border-red-400 text-red-700';
        messageDiv.textContent = 'Error resending OTP. Please try again.';
        messageDiv.classList.remove('hidden');
      } finally {
        resendBtn.disabled = false;
        resendBtn.innerHTML = originalText;
      }
    });

    document.getElementById('backToEmailBtn')?.addEventListener('click', function() {
      document.getElementById('emailStep').classList.remove('hidden');
      document.getElementById('otpStep').classList.add('hidden');
      document.getElementById('otpInput').value = '';
      document.getElementById('patientMessage').classList.add('hidden');
    });

    // Allow only numbers in OTP input
    document.getElementById('otpInput')?.addEventListener('input', function(e) {
      this.value = this.value.replace(/[^0-9]/g, '');
    });

    function GoogleAuth() {
        window.location.href="http://localhost:4040/patientPage/auth/google"
      }
  </script>
</body>
